{
  "component": {
    "prefix": "rnc",
    "body": [
      "import React, { Component } from 'react';",
      "",
      "import { View } from 'react-native';",
      "",
      "// import styles from './styles';",
      "",
      "export default class ${1:${TM_DIRECTORY/^.+\\/(.*)$/$1/}} extends Component {",
      "  render() {",
      "    return <View />;",
      "  }",
      "}",
      ""
    ],
    "description": "Create react-native component"
  },

  "componentStateless": {
