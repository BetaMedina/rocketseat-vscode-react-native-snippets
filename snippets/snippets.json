      "import { View } from 'react-native';",
      "",
      "// import styles from './styles';",
      "",
      "const ${1:${TM_DIRECTORY/^.+\\/(.*)$/$1/}} = () => <View />;",
      "",
      "export default ${1:${TM_DIRECTORY/^.+\\/(.*)$/$1/}};",
      ""
    ],
    "description": "Create react-native stateless component"
  },

  "styles": {
